{
    "webserver": {
        "views/schema-validation": {
            "view": "fostgres.sql",
            "configuration": {
                "sql": [
                    {
                        "return": "object",
                        "path": [1],
                        "GET": "SELECT schema as __ FROM json_schema WHERE slug=$1",
                        "PUT": {
                            "table": "json_schema",
                            "columns": {
                                "slug": {
                                    "key": true,
                                    "source": 1
                                },
                                "schema": {
                                    "source": ["body"]
                                }
                            }
                        }
                    }
                ]
            }
        }
    }
}
